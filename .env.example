# NetListLab / BuildFolio â€” Environment Variables

# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Database (Prisma)
DATABASE_URL=postgresql://user:password@localhost:5432/buildfolio?schema=public

# Server
PORT=4000
NODE_ENV=development

# JWT (Supabase handles this, but for reference)
JWT_SECRET=your_jwt_secret

# File Upload
MAX_FILE_SIZE_MB=10
ALLOWED_FILE_TYPES=image/png,image/jpeg,image/webp,application/pdf

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
